{
	"window.zoomLevel": 1,
	"workbench.startupEditor": "none",
	"workbench.editor.labelFormat": "short",
	"workbench.colorTheme": "Developers Theme - Slack Dark",
	"workbench.iconTheme": "developer-icon-theme",
	"workbench.productIconTheme": "developer-icons",
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#1a1a1a",
		"titleBar.activeForeground": "#c7c4c3",
		"activityBar.background": "#121212",
		"activityBar.inactiveForeground": "#3a3a3a",
		"activityBar.border": "#ffffff00",
		"sideBar.background": "#252525",
		"sideBar.border": "#252525",
		"statusBar.border": "#00000000",
		"statusBar.focusBorder": "#00000000",
		"statusBarItem.hoverForeground": "#ffffff",
		"list.inactiveSelectionBackground": "#343434",
		"list.activeSelectionBackground": "#343434",
		"list.focusAndSelectionOutline": "#343434",
		//-----------------SVELTE-----------------
		"activityBar.foreground": "#da7056",
		"activityBar.activeBorder": "#da7056",
		"activityBarBadge.background": "#da7056",
		"statusBarItem.remoteBackground": "#da7056",
		"statusBarItem.hoverBackground": "#da7056",
		"tab.activeBorderTop": "#da7056",
		"terminal.tab.activeBorder": "#da7056"
		//-----------------VUE-----------------
		// "activityBar.foreground": "#07b76e",
		// "activityBar.activeBorder": "#07b76e",
		// "activityBarBadge.background": "#07b76e",
		// "statusBarItem.remoteBackground": "#07b76e",
		// "statusBarItem.hoverBackground": "#07b76e",
		// "tab.activeBorderTop": "#07b76e",
		// "terminal.tab.activeBorder": "#07b76e"
	},
	"editor.accessibilitySupport": "off",
	"editor.fontFamily": "'FiraCode Nerd Font', monospace",
	"editor.fontSize": 14,
	"editor.formatOnSave": true,
	"editor.minimap.enabled": false,
	"editor.linkedEditing": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"explorer.compactFolders": false,
	"breadcrumbs.enabled": false,
	"prettier.configPath": ".prettierrc",
	"scss.lint.unknownAtRules": "ignore",
	"css.lint.unknownAtRules": "ignore",
	"css.lint.emptyRules": "ignore",
	"scss.lint.emptyRules": "ignore",
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		".svelte-kit": true,
		".vscode": true,
		"node_modules": true,
		"README.md": true,
		".eslintignore": true,
		".eslintrc.cjs": true,
		".npmrc": true,
		".prettierignore": true,
		".prettierrc": true,
		"bun.lockb": true,
		"postcss.config.js": true,
		"vite.config.ts": true,
		".dockerignore": true,
		".gitignore": true,
		"tsconfig.json": true
	},
	"explorerExclude.backup": {}
}
